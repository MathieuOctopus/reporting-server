      <Query>
        <DataSourceName>redmine</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@project_list">
            <Value>=Parameters!project_list.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@priorities_list">
            <Value>=Parameters!priorities_list.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@tracker_list">
            <Value>=Parameters!tracker_list.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@users_list">
            <Value>=Parameters!users_list.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@finish_date">
            <Value>=Parameters!finish_date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     issue_list.*
FROM         issue_list
where days_to_resolve &gt; days_to_resolve_by_contract and
project_name not like 'BOSS%' and
project_name not like 'УСО%' and
project_name not like 'Redmine%' and
project_name in (@project_list) and
priority in (@priorities_list) and
tracker_name in (@tracker_list) and
created_on_2 &gt;= @start_date and
created_on_2 &lt;=@finish_date and
aggigned in (@users_list) 
order by (days_to_resolve - days_to_resolve_by_contract) desc</CommandText>
      </Query>
